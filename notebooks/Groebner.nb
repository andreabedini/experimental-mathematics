Cell["Multivariate polynomial reduction", "Title",
 CellChangeTimes->{{3.605217689283629*^9, 3.605217716518887*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"myPolynomialReduce", "[", 
   RowBox[{"f_", ",", "F__"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"u", ",", 
      RowBox[{"h", "=", "f"}], ",", 
      RowBox[{"r", "=", "0"}], ",", "i", ",", "q"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "This", " ", "is", " ", "the", " ", "table", " ", "containing", " ", 
      "the", " ", "reminders"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"u", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"Length", "[", "F", "]"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "We", " ", "need", " ", "to", " ", "use", " ", "SameQ", " ", "rather", 
       " ", "than", " ", "Equal"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"!", 
        RowBox[{"h", "~", "SameQ", "~", "0"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{
         "iterate", " ", "through", " ", "the", " ", "elements", " ", "of", 
          " ", "F"}], ",", " ", 
         RowBox[{
         "looking", " ", "for", " ", "an", " ", "element", " ", "whose", " ", 
          "leading", " ", "term", " ", "divides", " ", "f"}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", "=", "1"}], ",", 
          RowBox[{"i", "\[LessEqual]", 
           RowBox[{"Length", "[", "F", "]"}]}], ",", 
          RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"lt", "[", 
              RowBox[{"F", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "]"}], "~", "divides", "~", 
             RowBox[{"lt", "[", "h", "]"}]}], ",", 
            RowBox[{"Break", "[", "]"}]}], "]"}]}], "\[IndentingNewLine]", 
         "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{"if", " ", "i"}], "=", 
          RowBox[{
          "n", " ", "we", " ", "checked", " ", "all", " ", "terms", " ", 
           "and", " ", "none", " ", "satisfied", " ", "the", " ", "above", 
           " ", 
           RowBox[{"condition", "."}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", "\[LessEqual]", 
           RowBox[{"Length", "[", "F", "]"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"q", "=", 
            RowBox[{
             RowBox[{"lt", "[", "h", "]"}], "/", 
             RowBox[{"lt", "[", 
              RowBox[{"F", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "]"}]}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"u", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "+=", "q"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"h", "=", 
            RowBox[{
             RowBox[{"h", "-", 
              RowBox[{"q", " ", 
               RowBox[{"F", "[", 
                RowBox[{"[", "i", "]"}], "]"}]}]}], "//", "Expand"}]}]}], ",",
           "\[IndentingNewLine]", 
          RowBox[{"(*", " ", "else", " ", "*)"}], "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"r", "+=", 
            RowBox[{"lt", "[", "h", "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"h", "-=", 
            RowBox[{"lt", "[", "h", "]"}]}]}]}], "\[IndentingNewLine]", 
         "]"}]}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"{", 
      RowBox[{"u", ",", "r"}], "}"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.604887131925324*^9, 3.604887209147736*^9}, {
